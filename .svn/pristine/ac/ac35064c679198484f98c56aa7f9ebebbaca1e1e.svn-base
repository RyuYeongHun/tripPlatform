package joeun.project.dto;

public class ReferChoiceDto {
	
	private Integer choice_id;
	private String user_id;
	private Integer period_id;
	private String goal_name;
	private String name;
	private Integer choiceage_id;
	private Integer choicetheme_id;
	
	public ReferChoiceDto() {}

	public ReferChoiceDto(Integer choice_id, String user_id, Integer period_id, String goal_name, String name,
			Integer choiceage_id, Integer choicetheme_id) {
		super();
		this.choice_id = choice_id;
		this.user_id = user_id;
		this.period_id = period_id;
		this.goal_name = goal_name;
		this.name = name;
		this.choiceage_id = choiceage_id;
		this.choicetheme_id = choicetheme_id;
	}

	public Integer getChoice_id() {
		return choice_id;
	}

	public void setChoice_id(Integer choice_id) {
		this.choice_id = choice_id;
	}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public Integer getPeriod_id() {
		return period_id;
	}

	public void setPeriod_id(Integer period_id) {
		this.period_id = period_id;
	}

	public String getGoal_name() {
		return goal_name;
	}

	public void setGoal_name(String goal_name) {
		this.goal_name = goal_name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Integer getChoiceage_id() {
		return choiceage_id;
	}

	public void setChoiceage_id(Integer choiceage_id) {
		this.choiceage_id = choiceage_id;
	}

	public Integer getChoicetheme_id() {
		return choicetheme_id;
	}

	public void setChoicetheme_id(Integer choicetheme_id) {
		this.choicetheme_id = choicetheme_id;
	}

	@Override
	public String toString() {
		return "ReferChoiceDto [choice_id=" + choice_id + ", user_id=" + user_id + ", period_id=" + period_id
				+ ", goal_name=" + goal_name + ", name=" + name + ", choiceage_id=" + choiceage_id + ", choicetheme_id="
				+ choicetheme_id + "]";
	}

	
	

	



}
