package joeun.project.dto;

public class DayAssignmentDto {
	
	private Integer day_id;         
	private Integer random_id;   
	private Integer day_number; 		
	private String 	assign_day;
	
	public DayAssignmentDto () {}

	public DayAssignmentDto(Integer day_id, Integer random_id, Integer day_number, String assign_day) {
		super();
		this.day_id = day_id;
		this.random_id = random_id;
		this.day_number = day_number;
		this.assign_day = assign_day;
	}

	public Integer getDay_id() {
		return day_id;
	}

	public void setDay_id(Integer day_id) {
		this.day_id = day_id;
	}

	public Integer getRandom_id() {
		return random_id;
	}

	public void setRandom_id(Integer random_id) {
		this.random_id = random_id;
	}

	public Integer getDay_number() {
		return day_number;
	}

	public void setDay_number(Integer day_number) {
		this.day_number = day_number;
	}

	public String getAssign_day() {
		return assign_day;
	}

	public void setAssign_day(String assign_day) {
		this.assign_day = assign_day;
	}

	@Override
	public String toString() {
		return "DayAssignmentDto [day_id=" + day_id + ", random_id=" + random_id + ", day_number=" + day_number
				+ ", assign_day=" + assign_day + "]";
	}

	

	
	
}
