package joeun.project.dto;

public class ReferenceAiDto {
	
	private String user_id;
	private Integer product_id;
	private String tour_goal;
	private String tour_period;
	private Integer age_id;
	private Integer location_id;
	private Integer theme_id;

	public ReferenceAiDto() {}

	public ReferenceAiDto(String user_id, Integer product_id, String tour_goal, String tour_period, Integer age_id,
			Integer location_id, Integer theme_id) {
		super();
		this.user_id = user_id;
		this.product_id = product_id;
		this.tour_goal = tour_goal;
		this.tour_period = tour_period;
		this.age_id = age_id;
		this.location_id = location_id;
		this.theme_id = theme_id;
	}

	public String getUser_id() {
		return user_id;
	}

	public void setUser_id(String user_id) {
		this.user_id = user_id;
	}

	public Integer getProduct_id() {
		return product_id;
	}

	public void setProduct_id(Integer product_id) {
		this.product_id = product_id;
	}

	public String getTour_goal() {
		return tour_goal;
	}

	public void setTour_goal(String tour_goal) {
		this.tour_goal = tour_goal;
	}

	public String getTour_period() {
		return tour_period;
	}

	public void setTour_period(String tour_period) {
		this.tour_period = tour_period;
	}

	public Integer getAge_id() {
		return age_id;
	}

	public void setAge_id(Integer age_id) {
		this.age_id = age_id;
	}

	public Integer getLocation_id() {
		return location_id;
	}

	public void setLocation_id(Integer location_id) {
		this.location_id = location_id;
	}

	public Integer getTheme_id() {
		return theme_id;
	}

	public void setTheme_id(Integer theme_id) {
		this.theme_id = theme_id;
	}

	@Override
	public String toString() {
		return "ReferenceAiDto [user_id=" + user_id + ", product_id=" + product_id + ", tour_goal=" + tour_goal
				+ ", tour_period=" + tour_period + ", age_id=" + age_id + ", location_id=" + location_id + ", theme_id="
				+ theme_id + "]";
	}

	

	
	

}
