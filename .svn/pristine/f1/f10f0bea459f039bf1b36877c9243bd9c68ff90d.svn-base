package joeun.project.dto;


public class YhBolgguri_CatDto {
	private int bolgguri_id;
	private String cat_name;
	
	public YhBolgguri_CatDto() {}

	public YhBolgguri_CatDto(int bolgguri_id, String cat_name) {
		super();
		this.bolgguri_id = bolgguri_id;
		this.cat_name = cat_name;
	}

	@Override
	public String toString() {
		return "Bolgguri_CatDto [bolgguri_id=" + bolgguri_id + ", cat_name=" + cat_name + "]";
	}

	public int getBolgguri_id() {
		return bolgguri_id;
	}

	public void setBolgguri_id(int bolgguri_id) {
		this.bolgguri_id = bolgguri_id;
	}

	public String getCat_name() {
		return cat_name;
	}

	public void setCat_name(String cat_name) {
		this.cat_name = cat_name;
	}
	
}
