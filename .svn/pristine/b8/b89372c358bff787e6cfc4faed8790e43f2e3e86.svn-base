package joeun.project.dto;

public class GpsPoint {
	private double x;
	private double y;
	private String title;
	public GpsPoint() {	}
	
	public GpsPoint(double x, double y) {
		super();
		this.x = x;
		this.y = y;
	}

	public GpsPoint(double x, double y, String title) {
		super();
		this.x = x;
		this.y = y;
		this.title = title;
	}
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	public double getY() {
		return y;
	}
	public void setY(double y) {
		this.y = y;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	@Override
	public String toString() {
		return "GpsPoint [x=" + x + ", y=" + y + ", title=" + title + "]";
	}



}
