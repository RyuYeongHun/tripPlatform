package joeun.project.dto;

public class TourGoalDto {
	
	private Integer goal_id;
	private String goal_name;

	public TourGoalDto() {}

	public TourGoalDto(Integer goal_id, String goal_name) {
		super();
		this.goal_id = goal_id;
		this.goal_name = goal_name;
	}

	

	public Integer getGoal_id() {
		return goal_id;
	}

	public void setGoal_id(Integer goal_id) {
		this.goal_id = goal_id;
	}

	public String getGoal_name() {
		return goal_name;
	}

	public void setGoal_name(String goal_name) {
		this.goal_name = goal_name;
	}

	@Override
	public String toString() {
		return "goalDto [goal_id=" + goal_id + ", goal_name=" + goal_name + "]";
	}

	
	

}
