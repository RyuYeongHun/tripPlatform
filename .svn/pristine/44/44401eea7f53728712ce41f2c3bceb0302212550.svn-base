     	});
     	
     	$('.map_search_area_container').mousemove(function(e){
     		if(!mapIsDown) return;
     		e.preventDefault();
     		const x = e.pageX - $(this).offset().left;
     		const walk = (x - mapStartX) * 2; // 스크롤 속도
     		$(this).scrollLeft(mapScrollLeft - walk);
     	});
	   	
