		// 지역 클릭 시 해당 지역 중심으로 이동하는 함수
		function setMapCenter(mapAreaCode) {
		    let lat = 36.4; // 디폴트 위도
		    let lng = 128.2; // 디폴트 경도
		    let level = 12;  // 디폴트 줌 레벨

		    switch(mapAreaCode) {
		        case '1':  // 서울
		            lat = 37.5663;
		            lng = 126.9779;
		            level = 9;
		            break;
		        case '2':  // 인천
		            lat = 37.4563;
		            lng = 126.7052;
		            level = 10;
		            break;
		        case '3':  // 대전
		            lat = 36.3504;
		            lng = 127.3845;
		            level = 9;
		            break;
		        case '4':  // 대구
		            lat = 35.8714;
		            lng = 128.6014;
		            level = 9;
		            break;
		        case '5':  // 광주
		            lat = 35.1595;
		            lng = 126.8526;
		            level = 9;
		            break;
		        case '6':  // 부산
		            lat = 35.1796;
		            lng = 129.0756;
		            level = 9;
		            break;
		        case '7':  // 울산
		            lat = 35.5384;
		            lng = 129.3114;
		            level = 9;
		            break;
		        case '8':  // 세종특별자치시
		            lat = 36.4801;
		            lng = 127.2890;
		            level = 9;
		            break;
		        case '31':  // 경기도
		            lat = 37.2752;
		            lng = 127.0092;
		            level = 11;
		            break;
		        case '32':  // 강원특별자치도
		            lat = 37.5794;
		            lng = 128.3236;
		            level = 11;
		            break;
		        case '33':  // 충청북도
		            lat = 36.6357;
		            lng = 127.4914;
		            level = 11;
		            break;
		        case '34':  // 충청남도
		            lat = 36.5184;
		            lng = 126.8000;
		            level = 11;
		            break;
		        case '35':  // 경상북도
		            lat = 36.5760;
		            lng = 128.5056;
		            level = 11;
		            break;
		        case '36':  // 경상남도
		            lat = 35.2383;
		            lng = 128.6923;
		            level = 11;
		            break;
		        case '37':  // 전북특별자치도
		            lat = 35.7175;
		            lng = 127.1530;
		            level = 11;
		            break;
		        case '38':  // 전라남도
		            lat = 34.8161;
		            lng = 126.4630;
		            level = 11;
		            break;
		        case '39':  // 제주도
		            lat = 33.4996;
		            lng = 126.5312;
		            level = 10;
		            break;
		        default:
		            // 디폴트 값 (경상북도 중심)
		            lat = 36.4;
		            lng = 128.2;
		            level = 12;
		            break;
		    }

		    console.log('지도 이동 ' + lat + ' ' + lng + ' ' + level);
		    // 여기서 lat, lng, level을 이용하여 지도 위치 및 줌 레벨 설정
		    map.setCenter(new kakao.maps.LatLng(lat, lng));
		    map.setLevel(level);
		}


