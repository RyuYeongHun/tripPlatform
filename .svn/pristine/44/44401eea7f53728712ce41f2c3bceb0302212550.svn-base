     	let mapScrollLeft;
     	
     	$('.map_search_area_container').mousedown(function(e){
     		mapIsDown = true;
     		mapStartX = e.pageX - $(this).offset().left;
     		mapScrollLeft = $(this).scrollLeft();
     	});
     	
     	$('.map_search_area_container').mouseup(function(){
     		mapIsDown = false;
     	})
     	     	
     	$('.map_search_area_container').mouseleave(function(){
